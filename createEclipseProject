catkin_make_isolated  --force-cmake -G"Eclipse CDT4 - Unix Makefiles"
ROOT=$PWD
source devel_isolated/setup.bash
cd build_isolated

originFolder=("osg_interactive_markers" "rudder_plugin" "underwater_vehicle_dynamics" "usv_sim"
"usv_tf"
"uwsim_osgbullet"
"visualization_osg"
"freefloating_gazebo"
"osg_markers"              
"sail_plugin"    
"usv_base_ctrl"               
"usv_sim_rviz"  
"uwsim"         
"uwsim_osgocean"
"liftdragww"           
"osg_utils"                
"underwater_sensor_msgs"  
"usv_navigation"
"usv_sim_test" 
"uwsim_bullet"
"uwsim_osgworks")

targetFolder=("visualization_osg/osg_interactive_markers" "rudder_plugin" "underwater_simulation/underwater_vehicle_dynamics" "usv_sim"
"usv_tf"
"uwsim_osgbullet"
"visualization_osg/visualization_osg"
"freefloating_gazebo"
"visualization_osg/osg_markers"              
"sail_plugin"    
"usv_base_ctrl"               
"usv_sim_rviz"  
"underwater_simulation/uwsim"         
"uwsim_osgocean"
"liftdragww"           
"visualization_osg/osg_utils"                
"underwater_simulation/underwater_sensor_msgs"  
"usv_navigation"
"usv_sim_test" 
"uwsim_bullet"
"uwsim_osgworks")
for ((x=0; x < ${#originFolder[*]}; x++)) ; do 
	echo ${originFolder[$x]} ; 
	awk -f $(rospack find mk)/eclipse.awk ${originFolder[$x]}/.project > ${originFolder[$x]}/.project_with_env 
        mv ${originFolder[$x]}/.project_with_env ../src/${targetFolder[$x]}/.project
done



